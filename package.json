    "@jsdotlua/react-roblox": "^17.1.0",
    "@jsdotlua/scheduler": "^17.1.0",
    "@jsdotlua/shared": "^17.1.0"
  },
  "devDependencies": {
    "@jsdotlua/jest": "^3.6.1-rc.2",
